\i0\i\f6  \i0\ulnone\strike0\par\pard\fi720\qj\stextflow0\sa60\sb60\b\fs24\f9 5.\b0\b\f0  \b0\b\f9 K\b0\b\f10 \u7871 \'2Et lu\u7853 \'2En\b0\b\f9   \b0\ulnone\strike0\par\pard\qj\stextflow0\sa60\sb120\f3 Trong b\'E0i b\'E1o n\'E0y, nh\'F3m t\'E1c gi\f4 \u7843 \'2E \u273 \'64\'E3 nh\u7853 \'2En th\u7845 \'2Ey r\u7857 \'2Eng c\u7843 \'2E b\u7889 \'2En h\'E3ng \u273 \'64i\u7879 \'2En tho\u7841 \'2Ei n\'F3i tr\'EAn \u273 \'64
\f3 \'E3 c\'F3 nh\f4 \u7919 \'2Eng m\u7913 \'2Ec th\'E0nh c\'F4ng kh\'E1c nhau trong vi\u7879 \'2Ec x\'E2y d\u7921 \'2Eng v\'E0 duy \f3 tr\'EC gi\'E1 tr\f4 \u7883 \'2E th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu c\u7911 \'2Ea h\u7885 \'2E trong con tim v\'E0 kh\u7889 \'2Ei \'F3c c\u7911 \'2Ea ng\u432 \'75\u7901 \'2Ei ti\'EAu d\'F9ng. Trong \u273 \'64\'F3, Nokia l\'E0 
\f3 n\f4 \u7893 \'2Ei b\u7853 \'2Et h\u417 \'6Fn c\u7843 \'2E. Ch\'EDnh gi\'E1 tr\u7883 \'2E th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu \u273 \'64\'E3 g\'F3p ph\u7847 \'2En quan tr\u7885 \'2Eng t\u7841 \'2Eo ra v\'E0 duy tr\'EC s\u7921 \'2E trung th\'E0nh th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu trong th\'F3i quen mua s\u7855 \'2Em c\u7911 \'2Ea ng\u432 \'75\u7901 \'2Ei ti\'EAu d\'F9ng Vi\u7879 \'2Et Nam \u273 \'64\u7889 \'2Ei v\u7899 \'2Ei 
\f3 s\f4 \u7843 \'2En ph\u7849 \'2Em \u273 \'64i\u7879 \'2En tho\u7841 \'2Ei di \u273 \'64\u7897 \'2Eng. N\'F3i c\'E1ch kh\'E1c gi\'E1 tr\u7883 \'2E th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu c\'F3 t\'E1c \u273 \'64\u7897 \'2Eng r\u7845 \'2Et l\u7899 \'2En \u273 \'64\u7871 \'2En 
\f3 h\'E0nh vi mua s\f4 \u7855 \'2Em c\u7911 \'2Ea ng\u432 \'75\u7901 \'2Ei ti\'EAu d\'F9ng. Do \u273 \'64\'F3, \u273 \'64\u7875 \'2E c\u7841 \'2Enh tranh th\'E0nh c\'F4ng h\u417 \'6Fn n\u7919 \'2Ea \u7903 \'2E th\u7883 \'2E tr\u432 \'75\u7901 \'2Eng Vi\u7879 \'2Et Nam, c\'E1c h\'E3ng ph\u7843 \'2Ei d\'E0nh nhi\u7873 \'2Eu th\u7901 \'2Ei gian v\'E0 ngu\u7891 \'2En l\u7921 \'2Ec h\u417 \'6Fn n\u7919 \'2Ea \u273 \'64\u7875 \'2E
\f3  ph\'E1t tri\f4 \u7875 \'2En gi\'E1 tr\u7883 \'2E th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu c\u7911 \'2Ea m\u7895 \'2Ei h\'E3ng:\f3   - \f4 Tr\u432 \'75\u7899 \'2Ec ti\'EAn, c\'E1c h\'E3ng n\'EAn t\u7853 \'2Ep trung c\u7843 \'2Ei thi\u7879 \'2En v\u7873 \'2E c\u7843 \'2Em nh\u7853 \'2En ch\u7845 \'2Et l\u432 \'75\u7907 \'2Eng v\'E0 gi\'E1 tr\u7883 \'2E 
\f3 c\f4 \u7911 \'2Ea th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu m\'ECnh v\'EC y\u7871 \'2Eu t\u7889 \'2E n\'E0y \u7843 \'2Enh h\u432 \'75\u7903 \'2Eng \u273 \'64\u7871 \'2En quy\u7871 \'2Et \u273 \'64\u7883 \'2Enh mua c\u7911 \'2Ea kh\'E1ch h\'E0ng. H\u417 \'6Fn n\u7919 \'2Ea, c\u7843 \'2Em nh\u7853 \'2En gi\'E1 tr\u7883 \'2E v\'E0 ch\u7845 \'2Et l\u432 \'75\u7907 \'2Eng c\'F3 th\u7875 \'2E g
\f3 i\'FAp doanh nghi\f4 \u7879 \'2Ep b\'E1n s\u7843 \'2En ph\u7849 \'2Em v\u7899 \'2Ei gi\'E1 \f3 cao c\f4 \u361 \'75ng nh\u432 \'75 gi\'FAp c\'E1c h\'E3ng d\u7877 \'2E d\'E0ng \u273 \'64\u432 \'75a c\'E1c d\'F2ng s\u7843 \'2En ph\u7849 \'2Em m\u7899 \'2Ei v\'E0o th\u7883 \'2E tr\u432 \'75\u7901 \'2Eng. \u272 \'D0\u7875 \'2E l\'E0m \u273 \'64\u432 \'75\u7907 \'2Ec \u273 \'64i\u7873 \'2Eu n\'E0y, c\'E1c h\'E3ng ph\u7843 \'2Ei kh\'F4ng ng\u7915 \'2Eng n\'E2ng cao ch\u7845 \'2Et l\u432 \'75\u7907 \'2Eng s\u7843 \'2En ph\u7849 \'2Em c\u361 \'75ng nh\u432 \'75 duy 
\f3 tr\'EC s\f4 \u7921 \'2E \u7893 \'2En \u273 \'64\u7883 \'2Enh ch\u7845 \'2Et l\u432 \'75\u7907 \'2Eng cao n\'E0y cho b\u7845 \'2Et k\u7923 \'2E d\'F2ng s\u7843 \'2En ph\u7849 \'2Em n\'E0o \u273 \'64\u432 \'75\u7907 \'2Ec tung ra th\u7883 \'2E tr\u432 \'75\u7901 \'2Eng.
\f3  - Ti\f4 \u7871 \'2Ep \u273 \'64\'F3, c\'E1c h\'E3ng ph\u7843 \'2Ei \u273 \'64\u7863 \'2Ec bi\u7879 \'2Et t\u7853 \'2Ep trung tinh l\u7921 \'2Ec c\u7911 \'2Ea m\'ECnh \u273 \'64\u7875 \'2E n\'E2ng cao y\u7871 \'2Eu t\u7889 \'2E t\'EDnh li\'EAn t\u432 \'75\u7903 \'2Eng th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu v\'EC y\u7871 \'2Eu t\u7889 \'2E n\'E0y c\'F3 \u7843 \'2Enh h\u432 \'75\u7903 \'2Eng c\u7921 \'2Ec k\u7923 \'2E l\u7899 \'2En \u273 \'64\u7871 \'2En h\'E0nh vi mua s\u7855 \'2Em 
\f3 c\f4 \u7911 \'2Ea ng\u432 \'75\u7901 \'2Ei ti\'EAu d\'F9ng. V\'ED d\u7909 \'2E, n\'F3i \u273 \'64\u7871 \'2En Nokia, ng\u432 \'75\u7901 \'2Ei ta \f3 s\f4 \u7869 \'2E ngh\u297 \'69 ngay \u273 \'64\u7871 \'2En ng\u432 \'75\u7901 \'2Ei s\u7917 \'2E d\u7909 \'2Eng s\u7869 \'2E l\'E0 c\'E1c doanh nh\'E2n th\'E0nh \u273 \'64\u7841 \'2Et ho\u7863 \'2Ec nh\u7919 \'2Eng ng\u432 \'75\u7901 \'2Ei thu\u7897 \'2Ec t\u7847 \'2Eng l\u7899 \'2Ep cao trong x\'E3 h\u7897 \'2Ei; hay 
\f3 Sony Ericsson g\f4 \u7855 \'2En li\u7873 \'2En v\u7899 \'2Ei th\u7871 \'2E h\u7879 \'2E tr\u7867 \'2E n\u259 \'61ng \u273 \'64\u7897 \'2Eng, s\'E1ng tao\u8230 \'85\f3  \f4 \u272 \'D0\u7863 \'2Ec bi\u7879 \'2Et, Motorola c\u7847 \'2En ph\u7843 \'2Ei \f3 
t\f4 \u7853 \'2Ep trung nhi\u7873 \'2Eu v\'E0o y\u7871 \'2Eu t\u7889 \'2E n\'E0y, v\'EC kh\'E1ch h\'E0ng ch\u432 \'75a c\u7843 \'2E\f3 m nh\f4 \u7853 \'2En \u273 \'64\u432 \'75\u7907 \'2Ec nhi\u7873 \'2Eu t\u7915 \'2E th\u432 \'75\u417 \'6Fng \f3 hi\f4 \u7879 \'2Eu Motorola \u273 \'64em l\u7841 \'2Ei cho h\u7885 \'2E.
\f3  \ulnone\strike0\par\pard\fi720\qj\stextflow0\sa60\sb60 - K\f4 \u7871 \'2E ti\u7871 \'2Ep, c\'E1c th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu \u272 \'D0TD\u272 \'D0 c\u7847 \'2En \u273 \'64\u7847 \'2Eu t\u432 \'75 \u273 \'64\u7875 \'2E truy\u7873 \'2En t\u7843 \'2Ei \u273 \'64\u432 \'75\u7907 \'2Ec c\'E1c th\'F4ng tin 
\f3 li\'EAn quan v\f4 \u7873 \'2E l\u7883 \'2Ech s\u7917 \'2E h\'ECnh th\'E0nh, ph\'E1t tri\u7875 \'2En, nh\u7919 \'2Eng s\u7921 \'2E ki\u7879 \'2En n\u7893 \'2Ei b\u7853 \'2Et c\u7911 \'2Ea h\'E3ng\u8230 \'85 \u273 \'64\u7871 \'2En v\u7899 \'2Ei ng\u432 \'75\u7901 \'2Ei ti\'EAu d\'F9ng. \u272 \'D0i\u7873 \'2Eu n\'E0y c\'F3 th\u7875 \'2E \u273 \'64\u432 \'75\u7907 \'2Ec
\f3  th\f4 \u7921 \'2Ec hi\u7879 \'2En th\'F4ng qua qu\u7843 \'2Eng c\'E1o tr\'EAn c\'E1c ph\u432 \'75\u417 \'6Fng \ulnone\strike0\par\pard\fi720\qj\stextflow0\sa60\sb60\f3 ti\f4 \u7879 \'2En th\'F4ng tin \u273 \'64\u7841 \'2Ei ch\'FAng c\u361 \'75ng nh\u432 \'75 t\'E0i tr\u7907 \'2E c\'E1c s\u7921 \'2E ki\u7879 \'2En th\u7875 \'2E thao, ca nh\u7841 \'2Ec\u8230 \'85 V\'ED d\u7909 \'2E nh\u432 \'75 
\f3 Motorola n\'EAn t\'E0i tr\f4 \u7907 \'2E c\'E1c ch\u432 \'75\u417 \'6Fng tr\'ECnh ca nh\u7841 \'2Ec, trao c\'E1c su\u7845 \'2Et h\u7885 \'2Ec b\u7893 \'2Eng cho sinh vi\'EAn ngh\'E8o\u8230 \'85 \u273 \'64\u7875 \'2E t\u7841 \'2Eo d\u7921 \'2Eng n\'EAn h\'ECnh \u7843 \'2Enh t\u7889 \'2Et \u273 \'64\u7865 \'2Ep c\u361 \'75ng nh\u432 \'75 \u273 \'64\u432 \'75a th\'EAm th\'F4ng tin c\u7911 \'2Ea h\'E3ng v\'E0o 
\f3 trong t\'E2m th\f4 \u7913 \'2Ec kh\'E1ch h\'E0ng.\f3  - Cu\f4 \u7889 \'2Ei c\'F9ng, c\'E1c h\'E3ng \u272 \'D0TD\u272 \'D0 kh\'F4ng th\u7875 \'2E c\u7841 \'2Enh tranh th\'E0nh c\'F4ng tr\'EAn th\u7883 \'2E tr\u432 \'75\u7901 \'2Eng \f3 Vi\f4 \u7879 \'2Et Nam v\u7899 \'2Ei kh\'F4ng s\u7921 \'2E hi\u7875 \'2Eu bi\u7871 \'2Et s\'E2u s\u7855 \'2Ec v\u7873 \'2E v\u259 \'61n h\'F3a, x\'E3 h\u7897 \'2Ei v\'E0 con ng\u432 \'75\u7901 \'2Ei Vi\u7879 \'2Et Nam, v\'EC \u273 \'64\'E2y l\'E0 y\u7871 \'2Eu t\u7889 \'2E c\'F3 t\'E1c \u273 \'64\u7897 \'2Eng m\u7841 \'2Enh
\f3  m\f4 \u7869 \'2E \u273 \'64\u7871 \'2En h\'E0nh vi c\u7911 \'2Ea ng\u432 \'75\u7901 \'2Ei ti\'EAu d\'F9ng. V\'EC l\u7869 \'2E \u273 \'64\'F3, c\'E1c th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu \u272 \'D0TD\u272 \'D0 c\u7847 \'2En d\'E0nh nhi\u7873 \'2Eu ngu\u7891 \'2En l\u7921 \'2Ec \u273 \'64\u7875 \'2E nghi\'EAn c\u7913 \'2Eu c\'E1c y\u7871 \'2Eu t\u7889 \'2E v\u259 \'61n h\'F3a
\f3 - x\'E3 h\f4 \u7897 \'2Ei, \f3 th\'F3i quen ti\'EAu d\'F9ng c\f4 \u7911 \'2Ea ng\u432 \'75\u7901 \'2Ei Vi\u7879 \'2Et Nam \u273 \'64\u7875 \'2E \u273 \'64\u432 \'75a ra c\'E1c ch\'EDnh s\'E1ch Marketing h\u7907 \'2Ep l\'FD \f3 nh\f4 \u7845 \'2Et. V\'ED d\u7909 \'2E, c\'E1c h\'E3ng n\'EAn nghi\'EAn c\u7913 \'2Eu c\'E1c
\f3  y\f4 \u7871 \'2Eu t\u7889 \'2E VH\f3 -\f4 XH \u273 \'64\u7889 \'2Ei v\u7899 \'2Ei t\u7915 \'2Eng mi\u7873 \'2En ri\'EAng bi\u7879 \'2Et \f3 B\f4 \u7855 \'2Ec, Trung, Nam hay y\u7871 \'2Eu t\u7889 \'2E t\'E2m l\'FD c\u7911 \'2Ea t\u7915 \'2Eng nh\'F3m tu\u7893 \'2Ei\u8230 \'85 \u273 \'64\u7875 \'2E ho\u7841 \'2Ech \u273 \'64\u7883 \'2Enh chi\u7871 \'2En l\u432 \'75\u7907 \'2Ec 
\f3 ho\f4 \u7863 \'2Ec \u273 \'64\u432 \'75a ra ch\'EDnh s\'E1ch ph\'E1t tri\u7875 \'2En th\u432 \'75\u417 \'6Fng hi\u7879 \'2Eu, ch\'EDnh s\'E1ch b\'E1n h\'E0ng ph\'F9 h\u7907 \'2Ep v\u7899 \'2Ei t\u7915 \'2Eng th\u7883 \'2E tr\u432 \'75\u7901 \'2Eng m\u7909 \'2Ec ti\'EAu.
